{
    "name"       : "zoopcommerce/maggott-module",
    "version"    : "1.0",
    "description": "Zend Framework 2 Module that provides exception handling functionality",
    "type"       : "library",
    "license"    : "MIT",
    "keywords": [
        "doctrine",
        "exception",
        "json",
        "zf2",
        "shard"
    ],
    "homepage"   : "http://zoopcommerce.github.io/maggott-module",
    "authors"    : [
        {
            "name"  : "Tim Roediger",
            "email" : "superdweebie@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "zendframework/zend-http"                 : "~2.2",
        "zendframework/zend-mvc"                  : "~2.2",
        "zendframework/zend-servicemanager"       : "~2.2",
        "zendframework/zend-view"                 : "~2.2"
    },
    "suggest": {
        "zoopcommerce/shard-module": "Can be used for exception logging"
    },
    "autoload": {
        "psr-0": {
            "Zoop\\MaggottModule": "src/"
        }
    }
}